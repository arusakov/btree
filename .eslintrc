{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "block-scoped-var": 2,
    "consistent-return": 2,
    "curly": 2,
    "eqeqeq": 2,
    "global-require": 2,
    "no-else-return": 2,
    "no-restricted-globals": 2,
    "no-shadow": 2,
    "no-undef": 2,
    "radix": 2,
    "semi": 2
  }
}
